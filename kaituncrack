							if Modstween then Modstween:Stop() end wait(.5)
							game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("requestEntrance",Vector3.new(3864.8515625, 6.6796875, -1926.7841796875))
						elseif OldWorld and (Name == "God's Guard [Lv. 450]" or Name == "Sky Bandit [Lv. 150]" or Name == "Dark Master [Lv. 175]") and (CFrameMon.Position - game:GetService("Players").LocalPlayer.Character:WaitForChild("HumanoidRootPart").Position).magnitude > 3000 then
							if Modstween then Modstween:Stop() end wait(.5)
							game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("requestEntrance", Vector3.new(-4607.8227539063, 872.54248046875, -1667.5568847656))
						elseif OldWorld and (Name == "Shanda [Lv. 475]" or Name == "Royal Squad [Lv. 525]"or Name == "Royal Soldier [Lv. 550]") and (CFrameMon.Position - game:GetService("Players").LocalPlayer.Character:WaitForChild("HumanoidRootPart").Position).magnitude > 5000 then
							if Modstween then Modstween:Stop() end wait(.5)
							game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("requestEntrance", Vector3.new(-7894.6176757813, 5547.1416015625, -380.29119873047))
						elseif OldWorld and string.find(Name, "Ship") and (CFrameMon.Position - game:GetService("Players").LocalPlayer.Character:WaitForChild("HumanoidRootPart").Position).magnitude > 30000 then
							if Modstween then Modstween:Stop() end
							game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("requestEntrance",Vector3.new(923.21252441406, 126.9760055542, 32852.83203125))
						elseif OldWorld and not string.find(Name, "Ship") and (CFrameMon.Position - game:GetService("Players").LocalPlayer.Character:WaitForChild("HumanoidRootPart").Position).magnitude > 30000 then
							if Modstween then Modstween:Stop() end
							game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("requestEntrance",Vector3.new(-6508.5581054688, 89.034996032715, -132.83953857422))
						elseif game.Players.LocalPlayer:DistanceFromCharacter(CFrameMon.Position) > 3500 then
							if Modstween then Questtween:Stop() end
							if game.Players.LocalPlayer.Character:WaitForChild("Humanoid"):GetState() == Enum.HumanoidStateType.Dead then return end
							game:GetService("Players").LocalPlayer.Character:WaitForChild("Humanoid"):ChangeState(15)
							wait(.1)
							if FindBoss == false then
								game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrameMon
							end
							game.ReplicatedStorage.Remotes.CommF_:InvokeServer("SetSpawnPoint")
							if FindBoss == false then
								game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrameMon
							end
							game.ReplicatedStorage.Remotes.CommF_:InvokeServer("SetSpawnPoint")
							if FindBoss == false then
								game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrameMon
							end
							game.ReplicatedStorage.Remotes.CommF_:InvokeServer("SetSpawnPoint")
							repeat
								wait()
							until game:GetService("Players").LocalPlayer.Character:WaitForChild("Humanoid").Health > 0
							wait(.1)
						elseif (CFrameMon.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 150 then
							if Modstween then Modstween:Stop() end
							if FindBoss == false then
								game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrameMon
							end
						end 
					end
				end
			end
		end
	end)
end
CheckMastery = false
-- [AutofarmMastery Fruit]
function CheckfarmMastery()
	if CheckMastery == false then
		pcall(function()
		if game.Players.LocalPlayer.Data.DevilFruit.Value == "Flame-Flame" then
			MaxMastery = 100
		end
		if game.Players.LocalPlayer.Data.DevilFruit.Value == "Ice-Ice" then
			MaxMastery = 100
		end
		if game.Players.LocalPlayer.Data.DevilFruit.Value == "Light-Light" then
			MaxMastery = 110
		end
		if game.Players.LocalPlayer.Data.DevilFruit.Value == "Quake-Quake" then
			MaxMastery = 150
		end
		if game.Players.LocalPlayer.Data.DevilFruit.Value == "Dark-Dark" then
			MaxMastery = 110
		end
		if game.Players.LocalPlayer.Data.DevilFruit.Value == "String-String" then
			MaxMastery = 225
		end
		if game.Players.LocalPlayer.Data.DevilFruit.Value == "Rumble-Rumble" then
			MaxMastery = 250
		end
		if game.Players.LocalPlayer.Data.DevilFruit.Value == "Human-Human: Buddha" then
			MaxMastery = 250
		end
		if game.Players.LocalPlayer.Data.DevilFruit.Value == "Magma-Magma" then
			MaxMastery = 250
		end
		if game.Players.LocalPlayer.Data.DevilFruit.Value == "Sand-Sand" then
			MaxMastery = 250
		end
		if CombatFinish and game.Players.localPlayer.Data.Level.Value >= 2400 and game.Players.LocalPlayer.Backpack[game.Players.localPlayer.Data.DevilFruit.Value].Level.Value <= MaxMastery then
			AnimateText("Unique Hub | Autofarm Mastery! | "..game.Players.localPlayer.Data.DevilFruit.Value)
			AutofarmLevel = false
			Mastery = true
		end
		if CombatFinish and game.Players.localPlayer.Data.Level.Value >= 2400 and game.Players.LocalPlayer.Backpack[game.Players.localPlayer.Data.DevilFruit.Value].Level.Value >= MaxMastery then
			AnimateText("Unique Hub | Autofarm Mastery! | Success!")
			CheckMastery = true
			Mastery = false
		end
		end)
	end
end

SkillZ = true
SkillX = true
SkillC = true
SkillV = true
function AutoFarmMasteryDevilFruit()
	pcall(function()
	AutofarmLeve = false
	FastAttack = false
	if not game.Players.LocalPlayer.Character:FindFirstChild("HasBuso") then
		game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Buso")
	end
	if game:GetService("Workspace").Enemies:FindFirstChild(Name) then
		for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
			if Mastery and v.Name == Name and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
				repeat wait()
					pcall(function()
					FarmtoTarget = toTarget(v.HumanoidRootPart.Position,v.HumanoidRootPart.CFrame * CFrame.new(0,30,0))
					if v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 150 then
						if FarmtoTarget then FarmtoTarget:Stop() end
						StartMagnet = true
						PosMon = v.HumanoidRootPart.CFrame
						HealthMin = v.Humanoid.MaxHealth * 15/100
						if v.Humanoid.Health <= HealthMin and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
							if game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool") then
								if not game:GetService("Players").LocalPlayer.Character:FindFirstChild(game.Players.LocalPlayer.Data.DevilFruit.Value) then
									EquipWeapon(game.Players.LocalPlayer.Data.DevilFruit.Value)
								else
									PositionSkillMasteryDevilFruit = v.HumanoidRootPart.Position
									UseSkillMasteryDevilFruit = true
									if game:GetService("Players").LocalPlayer.Character:FindFirstChild(game.Players.LocalPlayer.Data.DevilFruit.Value) then
										MasteryDevilFruit = require(game:GetService("Players").LocalPlayer.Character[game.Players.LocalPlayer.Data.DevilFruit.Value].Data)
										DevilFruitMastery = game:GetService("Players").LocalPlayer.Character[game.Players.LocalPlayer.Data.DevilFruit.Value].Level.Value
									elseif game:GetService("Players").LocalPlayer.Backpack:FindFirstChild(game.Players.LocalPlayer.Data.DevilFruit.Value) then
										MasteryDevilFruit = require(game:GetService("Players").LocalPlayer.Backpack[game.Players.LocalPlayer.Data.DevilFruit.Value].Data)
										DevilFruitMastery = game:GetService("Players").LocalPlayer.Backpack[game.Players.LocalPlayer.Data.DevilFruit.Value].Level.Value
									end
									if game:GetService("Players").LocalPlayer.Character:FindFirstChild("Dragon-Dragon") then
										if SkillZ and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and DevilFruitMastery >= MasteryDevilFruit.Lvl.Z then
											game:service('VirtualInputManager'):SendKeyEvent(true, "Z", false, game)
											wait()
											game:service('VirtualInputManager'):SendKeyEvent(false, "Z", false, game)
											wait(.1)
										end
										if SkillX and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and DevilFruitMastery >= MasteryDevilFruit.Lvl.X then
											game:service('VirtualInputManager'):SendKeyEvent(true, "X", false, game)
											wait()
											game:service('VirtualInputManager'):SendKeyEvent(false, "X", false, game)
											wait(.1)
										end   
									elseif game:GetService("Players").LocalPlayer.Character:FindFirstChild("Human-Human: Buddha") then
										if SkillZ and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and game.Players.LocalPlayer.Character.HumanoidRootPart.Size == Vector3.new(7.6, 7.676, 3.686) and DevilFruitMastery >= MasteryDevilFruit.Lvl.Z then
										else
											game:service('VirtualInputManager'):SendKeyEvent(true, "Z", false, game)
											wait()
											game:service('VirtualInputManager'):SendKeyEvent(false, "Z", false, game)
											wait(.1)
										end
										if SkillX and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and DevilFruitMastery >= MasteryDevilFruit.Lvl.X then
											game:service('VirtualInputManager'):SendKeyEvent(true, "X", false, game)
											wait()
											game:service('VirtualInputManager'):SendKeyEvent(false, "X", false, game)
											wait(.1)
										end
										if SkillC and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and DevilFruitMastery >= MasteryDevilFruit.Lvl.C then
											game:service('VirtualInputManager'):SendKeyEvent(true, "C", false, game)
											wait()
											game:service('VirtualInputManager'):SendKeyEvent(false, "C", false, game)
											wait(.1)
										end  
									elseif game:GetService("Players").LocalPlayer.Character:FindFirstChild("Venom-Venom") then
										if SkillZ and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and DevilFruitMastery >= MasteryDevilFruit.Lvl.Z then
											game:service('VirtualInputManager'):SendKeyEvent(true, "Z", false, game)
											wait()
											game:service('VirtualInputManager'):SendKeyEvent(false, "Z", false, game)
											wait(.1)
										end
										if SkillX and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and DevilFruitMastery >= MasteryDevilFruit.Lvl.X then
											game:service('VirtualInputManager'):SendKeyEvent(true, "X", false, game)
											wait()
											game:service('VirtualInputManager'):SendKeyEvent(false, "X", false, game)
											wait(.1)
										end
										if SkillC and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and DevilFruitMastery >= MasteryDevilFruit.Lvl.C then
											game:service('VirtualInputManager'):SendKeyEvent(true, "C", false, game)
											wait()
											game:service('VirtualInputManager'):SendKeyEvent(false, "C", false, game)
											wait(.1)
										end
									elseif game:GetService("Players").LocalPlayer.Character:FindFirstChild(game.Players.LocalPlayer.Data.DevilFruit.Value) then
										game:GetService("Players").LocalPlayer.Character:FindFirstChild(game.Players.LocalPlayer.Data.DevilFruit.Value).MousePos.Value = v.HumanoidRootPart.Position
										if SkillZ and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and DevilFruitMastery >= MasteryDevilFruit.Lvl.Z then
											game:service('VirtualInputManager'):SendKeyEvent(true, "Z", false, game)
											wait()
											game:service('VirtualInputManager'):SendKeyEvent(false, "Z", false, game)
											wait(.1)
										end
										if SkillX and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and DevilFruitMastery >= MasteryDevilFruit.Lvl.X then
											game:service('VirtualInputManager'):SendKeyEvent(true, "X", false, game)
											wait()
											game:service('VirtualInputManager'):SendKeyEvent(false, "X", false, game)
											wait(.1)
										end
										if SkillC and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and DevilFruitMastery >= MasteryDevilFruit.Lvl.C then
											game:service('VirtualInputManager'):SendKeyEvent(true, "C", false, game)
											wait()
											game:service('VirtualInputManager'):SendKeyEvent(false, "C", false, game)
											wait(.1)
										end
										if SkillV and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and DevilFruitMastery >= MasteryDevilFruit.Lvl.V then
											game:service('VirtualInputManager'):SendKeyEvent(true, "V", false, game)
											wait()
											game:service('VirtualInputManager'):SendKeyEvent(false, "V", false, game)
											wait(.1)
										end
									end
								end
							end
						else
							if v:FindFirstChild("HumanoidRootPart") then
							game:GetService('VirtualUser'):CaptureController()
							game:GetService('VirtualUser'):ClickButton1(Vector2.new(851, 158), game:GetService("Workspace").Camera.CFrame)
							UseSkillMasteryDevilFruit = false
							if not game.Players.LocalPlayer.Character:FindFirstChild(_G.MeleeSelect) then
								wait()
								EquipWeapon(_G.MeleeSelect)
							end
							v.HumanoidRootPart.Size = Vector3.new(60,60,60)
							game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, 30, 0)
							end
						end
					end
					end)
				until not game:GetService("Workspace").Enemies:FindFirstChild(Name) or not Mastery or v.Humanoid.Health <= 0 or not v.Parent
				if game:GetService("Players").LocalPlayer.Character:FindFirstChild(game.Players.LocalPlayer.Data.DevilFruit.Value) then
					game:GetService("Players").LocalPlayer.Character:FindFirstChild(game.Players.LocalPlayer.Data.DevilFruit.Value).Parent = game:GetService("Players").LocalPlayer.Backpack
				end
			end
		end
	else
		StartMagnet = false
		Modstween = toTarget(CFrameMon.Position,CFrameMon)
		if OldWorld and (Name == "Fishman Commando [Lv. 400]" or Name == "Fishman Warrior [Lv. 375]") and (CFrameQuest.Position - game:GetService("Players").LocalPlayer.Character:WaitForChild("HumanoidRootPart").Position).magnitude > 50000 then
			if Modstween then Modstween:Stop() end wait(.5)
			game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("requestEntrance",Vector3.new(61163.8515625, 11.6796875, 1819.7841796875))
		elseif OldWorld and not (Name == "Fishman Commando [Lv. 400]" or Name == "Fishman Warrior [Lv. 375]") and (CFrameQuest.Position - game:GetService("Players").LocalPlayer.Character:WaitForChild("HumanoidRootPart").Position).magnitude > 50000 then
			if Modstween then Modstween:Stop() end wait(.5)
			game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("requestEntrance",Vector3.new(3864.8515625, 6.6796875, -1926.7841796875))
		elseif OldWorld and (Name == "God's Guard [Lv. 450]" or Name == "Sky Bandit [Lv. 150]" or Name == "Dark Master [Lv. 175]") and (CFrameQuest.Position - game:GetService("Players").LocalPlayer.Character:WaitForChild("HumanoidRootPart").Position).magnitude > 3000 then
			if Modstween then Modstween:Stop() end wait(.5)
			game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("requestEntrance", Vector3.new(-4607.8227539063, 872.54248046875, -1667.5568847656))
		elseif OldWorld and (Name == "Shanda [Lv. 475]" or Name == "Royal Squad [Lv. 525]"or Name == "Royal Soldier [Lv. 550]") and (CFrameQuest.Position - game:GetService("Players").LocalPlayer.Character:WaitForChild("HumanoidRootPart").Position).magnitude > 5000 then
			if Modstween then Modstween:Stop() end wait(.5)
			game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("requestEntrance", Vector3.new(-7894.6176757813, 5547.1416015625, -380.29119873047))
		elseif NewWorld and string.find(Name, "Ship") and (CFrameQuest.Position - game:GetService("Players").LocalPlayer.Character:WaitForChild("HumanoidRootPart").Position).magnitude > 30000 then
			if Modstween then Modstween:Stop() end
			game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("requestEntrance",Vector3.new(923.21252441406, 126.9760055542, 32852.83203125))
		elseif NewWorld and not string.find(Name, "Ship") and (CFrameQuest.Position - game:GetService("Players").LocalPlayer.Character:WaitForChild("HumanoidRootPart").Position).magnitude > 30000 then
			if Modstween then Modstween:Stop() end
			game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("requestEntrance",Vector3.new(-6508.5581054688, 89.034996032715, -132.83953857422))
		elseif (CFrameQuest.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 150 then
			if Modstween then Modstween:Stop() end wait(.5)
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrameMon
		end 
	end
	end)
end

spawn(function()
	local gg = getrawmetatable(game)
	local old = gg.__namecall
	setreadonly(gg,false)
	gg.__namecall = newcclosure(function(...)
		local method = getnamecallmethod()
		local args = {...}
		if tostring(method) == "FireServer" then
			if tostring(args[1]) == "RemoteEvent" then
				if tostring(args[2]) ~= "true" and tostring(args[2]) ~= "false" then
					if UseSkillMasteryDevilFruit and Mastery then
						if type(args[2]) == "vector" then 
							args[2] = PositionSkillMasteryDevilFruit
						else
							args[2] = CFrame.new(PositionSkillMasteryDevilFruit)
						end
						return old(unpack(args))
					end
				end
			end
		end
		return old(...)
	end)
end)

spawn(function()
	while wait() do
		if Mastery then
			CheckQuest()
			AutoFarmMasteryDevilFruit()
		end
	end
end)


-- [Awake Fruit]

spawn(function()
	while wait(.1) do
		if NextIsland then		
			if game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 5") and (game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 5").Position - game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 2500 then
				ToIsland5CFrame = game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 5").CFrame
				if (game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 5").Position - game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).magnitude > 300 then
					Farmtween = toTarget(ToIsland5CFrame)
				elseif (ToIsland5CFrame.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 300 then
					if Farmtween then Farmtween:Stop() end
					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = ToIsland5CFrame * CFrame.new(0, 30, 0)
				end
			elseif game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 4") and (game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 4").Position - game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 2500 then
				ToIsland4CFrame = game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 4").CFrame
				if (game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 4").Position - game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).magnitude > 300 then
					Farmtween = toTarget(ToIsland4CFrame)
				elseif (ToIsland4CFrame.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 300 then
					if Farmtween then Farmtween:Stop() end
					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = ToIsland4CFrame * CFrame.new(0, 30, 0)
				end
			elseif game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3") and (game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3").Position - game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 2500 then
				ToIsland3CFrame = game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3").CFrame
				if (game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3").Position - game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).magnitude > 300 then
					Farmtween = toTarget(ToIsland3CFrame)
				elseif (ToIsland3CFrame.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 300 then
					if Farmtween then Farmtween:Stop() end
					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = ToIsland3CFrame * CFrame.new(0, 30, 0)
				end
			elseif game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2") and (game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2").Position - game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 2500 then
				ToIsland2CFrame = game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2").CFrame
				if (game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2").Position - game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).magnitude > 300 then
					Farmtween = toTarget(ToIsland2CFrame)
				elseif (ToIsland2CFrame.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 300 then
					if Farmtween then Farmtween:Stop() end
					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = ToIsland2CFrame * CFrame.new(0, 30, 0)
				end
			elseif check == false then
				check = true
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, 60, 0)
			end
		end
	end
end)
spawn(function()
	while wait(.1) do
		if NextIsland then
			pcall(function()
				for i, v in pairs(game:GetService("Workspace")["_WorldOrigin"].Locations:GetChildren()) do
					if string.find(v.Name, "Island") and (v.Position - game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).magnitude >= 2500 then
						v:Destroy()
					end
				end
			end)
		end
	end
end)

spawn(function()
	while wait(.1) do
		if Awakener then
			pcall(function()
				game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Awakener","Check")
				wait()
				game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Awakener","Awaken")
			end)
		end
	end
end)

function CheckFruit()
	for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
		if v:IsA("Tool") and string.find(v.Name,"Fruit") then
			return true
		end
	end
	for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
		if v:IsA("Tool") and string.find(v.Name,"Fruit") then
			return true
		end
	end
end

function CheckAwakeSkill()
	AwakenedSkill = "nil"
	if game:GetService("Players").LocalPlayer.PlayerGui.Main.AwakeningToggler.Visible == true then
		wait(.4)
		for i,v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.Main.AwakeningToggler.TopContainer.Frame:GetChildren()) do
			if v.Name ~= "Template" and v.Name ~= "UIListLayout" then
				if v.Label.Text ~= "(Awakened)" then
					AwakenedSkill = v.Name
				end
			end
		end
	else
		wait(.3)
		game:GetService("Players").LocalPlayer.PlayerGui.Main.AwakeningToggler.Visible = true
	end
end


function CheckAwake()
	Player = game.Players.LocalPlayer
	if Player.Data.DevilFruit.Value == "Flame-Flame" then
		SelectChips = "Flame"
	end
	if Player.Data.DevilFruit.Value == "Ice-Ice" then
		SelectChips = "Ice"
	end
	if Player.Data.DevilFruit.Value == "Light-Light" then
		SelectChips = "Light"
	end
	if Player.Data.DevilFruit.Value == "Quake-Quake" then
		SelectChips = "Quake"
	end
	if Player.Data.DevilFruit.Value == "Dark-Dark" then
		SelectChips = "Dark"
	end
	if Player.Data.DevilFruit.Value == "String-String" then
		SelectChips = "String"
	end
	if Player.Data.DevilFruit.Value == "Rumble-Rumble" then
		SelectChips = "Rumble"
	end
	if Player.Data.DevilFruit.Value == "Human-Human: Buddha" then
		SelectChips = "Human: Buddha"
	end
	if Player.Data.DevilFruit.Value == "Magma-Magma" then
		SelectChips = "Magma"
	end
	if Player.Data.DevilFruit.Value == "Sand-Sand" then
		SelectChips = "Sand"
	end
	CheckAwakeSkill()
	wait(.3)
	if AwakenedSkill ~= "nil" and game.Players.LocalPlayer.Data.Level.Value >= 2400 and CheckMastery == true then
		AnimateText("Unique Hub | Mode : Awake!")
		AutofarmLevel = false
		Awakener = true
		AutoRaid()
	elseif AwakenedSkill == "nil" and game.Players.LocalPlayer.Data.Level.Value >= 2400 and CheckMastery == true then
		AnimateText("Unique Hub | Mode : Normal!")
		AutoRaidNotHop()
	end
end

CheckAwake()

local checkraid = false 
RaidStart = false
function AutoRaid()
    spawn(function()
        if not game.Players.LocalPlayer.PlayerGui.Main.Timer.Visible  then 
            cancel()
        end
    end)
    stupid = true 
	FastAttack = false
	if game.Players.LocalPlayer.Backpack:FindFirstChild("Special Microchip") or game.Players.LocalPlayer.Character:FindFirstChild("Special Microchip") then
		if game.Players.LocalPlayer.Backpack:FindFirstChild("Special Microchip") or game.Players.LocalPlayer.Character:FindFirstChild("Special Microchip") and game.Players.LocalPlayer.PlayerGui.Main.Timer.Visible == false then
			AnimateText("Unique Hub | Actived Raid!")
			if NewWorld then
				fireclickdetector(Workspace.Map.CircleIsland.RaidSummon2.Button.Main.ClickDetector)
				repeat wait() until game.Players.LocalPlayer.PlayerGui.Main.Timer.Visible == true 
				wait(.4)
			elseif ThirdWorld then
				fireclickdetector(Workspace.Map["Boat Castle"].RaidSummon2.Button.Main.ClickDetector)
				repeat wait() until game.Players.LocalPlayer.PlayerGui.Main.Timer.Visible == true 
				wait(.4)
			end
		end
		if game.Players.LocalPlayer.PlayerGui.Main.Timer.Visible == true then
			game:GetService("Players")["LocalPlayer"].PlayerGui.Main.AwakeningToggler.Visible = true
			AnimateText("Unique Hub | Auto Raid!")
			check = false
			repeat wait()
				Awakener = false
				NextIsland = true
				for i,v in pairs(game.Workspace.Enemies:GetChildren()) do
					if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0 and (v.HumanoidRootPart.Position-game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 1000 then
						InMyNetWork(v.HumanoidRootPart)
						v.HumanoidRootPart.CFrame = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0,-30,0)
						v.Humanoid:ChangeState(15)
					end
				end
			until game.Players.LocalPlayer.PlayerGui.Main.Timer.Visible == false
			game:GetService("Players")["LocalPlayer"].PlayerGui.Main.AwakeningToggler.Visible = false
			RaidStart = false
		end
	else
		FindFruit = false
		if CheckFruit() ~= true and not game.Players.LocalPlayer.Backpack:FindFirstChild("Special Microchip") and not game.Players.LocalPlayer.Character:FindFirstChild("Special Microchip")
		and game.Players.LocalPlayer.PlayerGui.Main.Timer.Visible == false then
			getgenv().Settings.AutoStoreFruit = false
			AnimateText("Unique Hub | LoadFruit!")
			for i,v in pairs(game:GetService("ReplicatedStorage").Remotes["CommF_"]:InvokeServer("getInventoryFruits")) do
				if v.Price < 1000000 then 
					FindFruit = true
					game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("LoadFruit",v.Name)
				end
			end
		end	
		if CheckFruit() == true and not game.Players.LocalPlayer.Backpack:FindFirstChild("Special Microchip") and not game.Players.LocalPlayer.Character:FindFirstChild("Special Microchip") 
		and game.Players.LocalPlayer.PlayerGui.Main.Timer.Visible == false then
			FindFruit = true
			AnimateText("Unique Hub | Buy Microchip!")
			local args = {
				[1] = "RaidsNpc",
				[2] = "Select",
				[3] = SelectChips
			}
			local CheckRaids = game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
			getgenv().Settings.AutoStoreFruit = true
		end
	end
	if FindFruit == false and CheckFruit() ~= true and game.Players.LocalPlayer.PlayerGui.Main.Timer.Visible == false and AwakeSucces ~= true then
		ServerHop()
		wait(3)
	end
end

function AutoRaidNotHop()
	FastAttack = false
	if game.Players.LocalPlayer.Backpack:FindFirstChild("Special Microchip") or game.Players.LocalPlayer.Character:FindFirstChild("Special Microchip") then
		AutofarmLevel = false
		if game.Players.LocalPlayer.Backpack:FindFirstChild("Special Microchip") or game.Players.LocalPlayer.Character:FindFirstChild("Special Microchip") and game.Players.LocalPlayer.PlayerGui.Main.Timer.Visible == false then
			AnimateText("Unique Hub | Actived Raid!")
			if NewWorld then
				fireclickdetector(Workspace.Map.CircleIsland.RaidSummon2.Button.Main.ClickDetector)
				repeat wait() until game.Players.LocalPlayer.PlayerGui.Main.Timer.Visible == true 
				wait(.4)
			elseif ThirdWorld then
				fireclickdetector(Workspace.Map["Boat Castle"].RaidSummon2.Button.Main.ClickDetector)
				repeat wait() until game.Players.LocalPlayer.PlayerGui.Main.Timer.Visible == true 
				wait(.4)
			end
		end
		if game.Players.LocalPlayer.PlayerGui.Main.Timer.Visible == true then
			game:GetService("Players")["LocalPlayer"].PlayerGui.Main.AwakeningToggler.Visible = true
			AnimateText("Unique Hub | Auto Raid!")
			check = false
			repeat wait()
				Awakener = false
				NextIsland = true
				for i,v in pairs(game.Workspace.Enemies:GetChildren()) do
					if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0 and (v.HumanoidRootPart.Position-game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 1000 then
						InMyNetWork(v.HumanoidRootPart)
						v.HumanoidRootPart.CFrame = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0,-30,0)
						v.Humanoid:ChangeState(15)
					end
				end
			until game.Players.LocalPlayer.PlayerGui.Main.Timer.Visible == false
			game:GetService("Players")["LocalPlayer"].PlayerGui.Main.AwakeningToggler.Visible = false
			RaidStart = false
		end
	else
		AutofarmLevel = true
		FindFruit = false
		if CheckFruit() ~= true and not game.Players.LocalPlayer.Backpack:FindFirstChild("Special Microchip") and not game.Players.LocalPlayer.Character:FindFirstChild("Special Microchip")
		and game.Players.LocalPlayer.PlayerGui.Main.Timer.Visible == false then
			getgenv().Settings.AutoStoreFruit = false
			AnimateText("Unique Hub | LoadFruit!")
			for i,v in pairs(game:GetService("ReplicatedStorage").Remotes["CommF_"]:InvokeServer("getInventoryFruits")) do
				if v.Price < 1000000 then 
					FindFruit = true
					game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("LoadFruit",v.Name)
				end
			end
		end	
		if CheckFruit() == true and not game.Players.LocalPlayer.Backpack:FindFirstChild("Special Microchip") and not game.Players.LocalPlayer.Character:FindFirstChild("Special Microchip") 
		and game.Players.LocalPlayer.PlayerGui.Main.Timer.Visible == false then
			FindFruit = true
			AnimateText("Unique Hub | Buy Microchip!")
			local args = {
				[1] = "RaidsNpc",
				[2] = "Select",
				[3] = SelectChips
			}
			local CheckRaids = game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
			getgenv().Settings.AutoStoreFruit = true
		end
	end
end


-- [Kaitan Setup , Autofarm loop]



function CommF_(...)
    return game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(...)
end

function Check_lvl_Combat(name,lvl) 
    wait(1)
    if not game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then return end ; 
    local plr = game.Players.LocalPlayer
    local Tool = nil ;

    for i ,v in pairs(plr.Character:GetChildren()) do 
        if v:IsA("Tool") and  v.ToolTip == "Melee" and( v.Name:match(name) or v.Name == name) then 
            Tool = v 
        end
    end
    for i ,v in pairs(plr.Backpack:GetChildren()) do 
        if v:IsA("Tool") and  v.ToolTip == "Melee" and( v.Name:match(name) or v.Name == name) then 
            Tool = v 
        end
    end

    if Tool == nil then 
        return true
    end

    if Tool then 
        if Tool.Level.Value < lvl then 
            return true
        else
            return false 
        end
    else
        return false 
    end

    
end

local plr = game.Players.LocalPlayer ; 

function SuperHuman() 
    if SuperData.Superhuman then return end ; 

    if CommF_("BuySuperhuman") > 0 then 
        SuperData.Superhuman = true 
        CombatData.SuperHuman = true 
        return
    end

    if not SuperData.Electro then 
        if CommF_("BuyElectro") ~= 0  then         
            if not Check_lvl_Combat("Electro",400)  then          
                SuperData.Electro = true 
                return
            else    
                return
            end
        end
        return ; 
    end
    if not SuperData.BlackLeg then   
        if CommF_("BuyBlackLeg") ~= 0  then 
            if not Check_lvl_Combat("Black Leg",400)  then 
                SuperData.BlackLeg = true 
            else
                return
            end
        end
        return
    end
    if not SuperData.FishmanKarate  then
        if CommF_("BuyFishmanKarate") ~= 0  then 
            if not Check_lvl_Combat("Fishman Karate",400)  then 
                SuperData.FishmanKarate = true 
            else
                return
            end
        end
        return
    end
    if not SuperData.DragonClaw  then
        if CommF_("BlackbeardReward", "DragonClaw", "2") ~= 0  then 
            if not Check_lvl_Combat("Dragon Claw",400)  then 
                SuperData.DragonClaw = true 
            else
                return
            end
        else
            AutoRaid()
            return true
        end
        return
    end
    if not SuperData.Superhuman then 
        if plr.Data.Beli.Value >= 3e+6 and CommF_("BuySuperhuman") == 0  then 
            SuperData.Superhuman = true 
            CombatData.SuperHuman = true 
            return
        end
        return ; 
    end

end

function CheckCombat()
    for i ,v in pairs(CombatData) do 
        if not v then return true end 
    end
    return false 
end;

local loadmons = function()
    for i ,v in pairs(game:GetService("ReplicatedStorage"):GetChildren()) do 
        if v:IsA("Model") and v.Name:match("Lv.") then 
            v.Parent = game:GetService("Workspace").Enemies
        end
    end
end

function N_C(p1) 
    local Char_Fol = game:GetService("Workspace").Characters ;
    if not Char_Fol:FindFirstChild(plr.Name) then return Char_Fol:WaitForChild(plr.Name) ; end 
    return Char_Fol[plr.Name]:FindFirstChild(p1) ; 
end ; 

function HasItems(NameItems) 
    local p1 = NameItems 
    if not N_C("Humanoid")  then return end ; 
    local plr = game.Players.LocalPlayer ; 

    for i ,v in pairs(plr.Character:GetChildren()) do 
        if v.Name == p1 or v.Name:match(p1) then 
            return true 
        end
    end
    for i ,v in pairs(plr.Backpack:GetChildren()) do 
        if v.Name == p1 or v.Name:match(p1)  then 
            return true 
        end
    end
 
    return false 
end

local Bones = false 

function AutoBones() 
    if Bones then 
        if not N_C("Humanoid")  then return end ; 
        
        local getcf = function()
            local list = {} 
            for i2 ,v2 in pairs(game:GetService("Workspace")["_WorldOrigin"].EnemySpawns:GetChildren()) do 
                if  (v2.Name:match("Reborn Skeleton") or v2.Name:match("Demonic Soul") or v2.Name:match("Posessed Mummy") or v2.Name:match("Living Zombie")) then 
                    table.insert(list,v2.CFrame) ;
                end
            end 
            return list 
        end


        for i ,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do 
            if (v.Name:match("Reborn Skeleton") or v.Name:match("Demonic Soul") or v.Name:match("Posessed Mummy") or v.Name:match("Living Zombie")) and v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0  then
                repeat wait() 
                    pcall(function()
                        if not game.Players.LocalPlayer.Character:FindFirstChild("HasBuso") then
                            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Buso")
                        end
                        if not game.Players.LocalPlayer.Character:FindFirstChild(_G.MeleeSelect) then
                            wait()
                            EquipWeapon(_G.MeleeSelect)
                        end
                        toTarget(v.HumanoidRootPart.CFrame * CFrame.new(1,20,0))
                        PosMon = v.HumanoidRootPart.CFrame
                        v.Humanoid.NameDisplayDistance = 0
                        v.HumanoidRootPart.Size = Vector3.new(80,80,80)
                        v.HumanoidRootPart.Transparency = 1
                        v.Humanoid.JumpPower = 0
                        v.Humanoid.WalkSpeed = 0
                        v.HumanoidRootPart.CanCollide = false
                        if v.Humanoid:FindFirstChild("Animator") then
                            v.Humanoid.Animator:Destroy()
                        end
                        v.Humanoid:ChangeState(11)
                        sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius",  math.huge)
                    end)
                until not v or not v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health <= 0 or not Bones
            end 
        end 

        if have == nil then 
            for i ,v in pairs(getcf()) do 
                toTarget(v.HumanoidRootPart.CFrame * CFrame.new(0,30,0))
            end
        end

    end
end

spawn(function()
    while true do wait() 
        AutoBones() ; 
    end
end)

function MakeCombat() 
    if not CheckCombat()  then CombatFinish = false return end ; 

    CombatFinish = true 
    if not CombatData.SuperHuman then 
        SuperHuman() 
        return ; 
    end

    if not CombatData.DarkStep   then 
        
        if  game.PlaceId ~= 4442272183 then return CommF_("TravelDressrosa")  end ; 

        if CommF_("BuyDeathStep") > 0 and CommF_("BuyDeathStep") < 3   then 
            CombatData.DarkStep = true 
            return false 
        end

        if plr.Data.Fragments.Value < 5000 then 
            return  AutoRaid()
        end

        if SuperData.BlackLeg and CommF_("OpenLibrary")  and plr.Data.Fragments.Value >= 5000 then 
            if  plr.Data.Beli.Value < 3e+6 then 
                return 
           end
            return CommF_("BuyDeathStep")
        end

        if  not CommF_("OpenLibrary") and  HasItems("Library Key") then 
            return  CommF_("OpenLibrary")
        end

        if  not CommF_("OpenLibrary") and  not HasItems("Library Key")  then 

            for i ,v in pairs(workspace.Enemies:GetChildren()) do 
                if (v.Name:match("Awakened Ice Admiral") or v.Name ==  "Snow Lurker [Lv. 1375]" or v.Name == "Arctic Warrior [Lv. 1350]" ) and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then 
                    repeat wait() 
                    pcall(function()
                        if not game.Players.LocalPlayer.Character:FindFirstChild("HasBuso") then
                            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Buso")
                        end
                         if not game.Players.LocalPlayer.Character:FindFirstChild(_G.MeleeSelect) then
                             wait()
                             EquipWeapon(_G.MeleeSelect)
                         end
                        toTarget(v.HumanoidRootPart.CFrame * CFrame.new(1,20,0))
                        PosMon = v.HumanoidRootPart.CFrame
                        v.Humanoid.NameDisplayDistance = 0
                        v.HumanoidRootPart.Size = Vector3.new(80,80,80)
                        v.HumanoidRootPart.Transparency = 1
                        v.Humanoid.JumpPower = 0
                        v.Humanoid.WalkSpeed = 0
                        v.HumanoidRootPart.CanCollide = false
                        if v.Humanoid:FindFirstChild("Animator") then
                            v.Humanoid.Animator:Destroy()
                        end
                        v.Humanoid:ChangeState(11)
                        sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius",  math.huge)
                    end)
                    until not v or not v:FindFirstChild("HumanoidRootPart") or v.Humanoid.Health <= 0 or HasItems("Library Key") ; 
                end
            end 

            return true 
        end ; 

        return true 
    end; 

    if not CombatData.SharkManV2 then 

        if  game.PlaceId ~= 4442272183 then print("Teleport") return CommF_("TravelDressrosa")  end ; 


        if type(CommF_("BuySharkmanKarate")) ~= "string" and CommF_("BuySharkmanKarate") > 0  and CommF_("BuySharkmanKarate") < 3   then 
            CombatData.SharkManV2  = true ; 
            return false ; 
        end

        if plr.Data.Fragments.Value >= 5000 then 
            if type(CommF_("BuySharkmanKarate",true)) ~= "string"  then 
               if  plr.Data.Beli.Value < 3e+6 then 
                    return 
               end
                 CommF_("BuySharkmanKarate")  ; 
                 CommF_("BuySharkmanKarate",true)  ;
                 return true  
            end ; 
        else
            return AutoRaid()
        end

        function find() 
            for i ,v in pairs(workspace.Enemies:GetChildren()) do 
                if v.Name:match("Tide Keeper") and v:FindFirstChild('HumanoidRootPart') and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then 
                    return true 
                end
            end
            return false 
        end


        if type(CommF_("BuySharkmanKarate",true)) == "string" and not HasItems("Water Key") then 

            if not find() then 
                return    pcall(TPReturner)
            end

            for i ,v in pairs(workspace.Enemies:GetChildren()) do 
                if v.Name:match("Tide Keeper") and v:FindFirstChild('HumanoidRootPart') and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then 
                    repeat wait() 
                        pcall(function()
                            if not game.Players.LocalPlayer.Character:FindFirstChild("HasBuso") then
                                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Buso")
                            end
                             if not game.Players.LocalPlayer.Character:FindFirstChild(_G.MeleeSelect) then
                                 wait()
                                 EquipWeapon(_G.MeleeSelect)
                             end
                            toTarget(v.HumanoidRootPart.CFrame * CFrame.new(1,20,0))
                            PosMon = v.HumanoidRootPart.CFrame
                            v.Humanoid.NameDisplayDistance = 0
                            v.HumanoidRootPart.Size = Vector3.new(80,80,80)
                            v.HumanoidRootPart.Transparency = 1
                            v.Humanoid.JumpPower = 0
                            v.Humanoid.WalkSpeed = 0
                            v.HumanoidRootPart.CanCollide = false
                            if v.Humanoid:FindFirstChild("Animator") then
                                v.Humanoid.Animator:Destroy()
                            end
                            v.Humanoid:ChangeState(11)
                            sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius",  math.huge)
                        end)
                        until not v or not v:FindFirstChild("HumanoidRootPart") or v.Humanoid.Health <= 0 or HasItems("Water Key"); 
                end
            end
            return true 
        end ; 




        return true 
    end

    if not CombatData.Electro then 

        if game.PlaceId ~= 7449423635 then return end ; 

        if  ( type(CommF_("BuyElectricClaw")) == "number" and CommF_("BuyElectricClaw") > 0 and  CommF_("BuyElectricClaw") < 3   ) then 
            CombatData.Electro = true ; 
            return ; 
        end

        if CommF_("BuyElectricClaw") == "..."   then

            if not plr.Data.Fragments.Value < 5000 then return AutoRaid() end ; 

            if plr.Data.Beli.Value < 3e+6 then return end ; 

            CommF_("BuyElectricClaw","Start")
            CommF_("requestEntrance",Vector3.new(-12462, 375, -7552))

            if type(CommF_("BuyElectricClaw") ) == "number" and CommF_("BuyElectricClaw")  > 0 then 
                CombatData.Electro = true 
                return true 
            end

            return true 
        end
        
        return true 
    end

    if not CombatData.Talon then 

        if type(CommF_("BuyDragonTalon")) == "number" and CommF_("BuyDragonTalon") > 0 and CommF_("BuyDragonTalon") < 3   then 
            CombatData.Talon  = true 
            return 
        end


        if not plr.Data.Fragments.Value < 5000 then return AutoRaid() end ; 

        if plr.Data.Beli.Value < 3e+6 then return end ; 

        if type(CommF_("BuyDragonTalon")) == "string" then 

            if not HasItems("Fire Essence") then 
                local Bone = CommF_("Bones","Buy",1,1)        
                if CommF_("Bones","Check") < 50 then 
                    Bones = true 
                else
                    Bones = false 
                end  
                return true 
            else
                CommF_("BuyDragonTalon",true) ; 
                if CommF_("BuyDragonTalon") ~= 0 and type(CommF_("BuyDragonTalon")) ~= "string" then 
                    CombatData.Talon = true 
                    return true 
                end
            end

        end


    end

end

spawn(function()
	while wait() do
		if game.Players.LocalPlayer.Data.Level.Value >= 2400 and CombatFinish then
			if getgenv().Settings.Kaitan.MasteryAndAwaking then
				CheckfarmMastery()
				if CheckMastery == true then -- มาสเสร็จ
					CheckAwake() -- ฟาร์มผลต่อ & ปิดเกม
				end
			elseif not getgenv().Settings.Kaitan.MasteryAndAwaking then
				wait(3)
				game:Shutdown() -- ปิดเกมหลังหมัดเสร็จ
			end
		end
		if AutofarmLevel and FindBoss == false then
            local stu = MakeCombat() ;  
			CheckQuest()
            if not stu then 
                stupid =false 
                AutoFarm()
            else
                stupid = true 
            end
		end
	end
end)

spawn(function()
    while true do 
        loadmons() 
        wait(5)
    end
end)

-- [ Magnet]

spawn(function()
	game:GetService("RunService").Heartbeat:Connect(function()
		pcall(function()
			if StartMagnet then
				for i,v in pairs(game.Workspace.Enemies:GetChildren()) do
					if not string.find(v.Name,"Boss") then
						if InMyNetWork(v.HumanoidRootPart) and v.Name ~= "Ice Admiral [Lv. 700] [Boss]" and v.Name ~= "Don Swan [Lv. 1000] [Boss]" and v.Name ~= "Saber Expert [Lv. 200] [Boss]" and v.Name ~= "Longma [Lv. 2000] [Boss]" then
							v.HumanoidRootPart.CFrame = PosMon
							v.Humanoid.NameDisplayDistance = 0
							v.Humanoid.JumpPower = 0
							v.Humanoid.WalkSpeed = 0
							v.HumanoidRootPart.CanCollide = false
							v.HumanoidRootPart.Size = Vector3.new(80,80,80)
							if v.Humanoid:FindFirstChild("Animator") then
								v.Humanoid.Animator:Destroy()
							end
							v.Humanoid:ChangeState(11)
						end
					end
				end
			end
		end)
	end)
end)

-- [Remove Mob]
spawn(function()
	while wait() do
		pcall(function()
			for i,v in pairs(game.Workspace.Enemies:GetChildren()) do
				if not string.find(v.Name,"Boss") and v.Humanoid.Health <= 0 and v:FindFirstChild("Humanoid") then
					v:Destroy()
				end
			end
		end)
	end
end)

-- [BoosAura]


FindBoss = false
spawn(function()
	while wait() do
		pcall(function()
            if stupid then return end 
			if getgenv().Settings.BossAura and (AutofarmLevel or FindMob == false) and not Mastery and not stupid then
				FindBoss = false
				if getgenv().Settings.TypeBossAura == "Around" then
					for i,v in pairs(game.Workspace.Enemies:GetChildren()) do
						if v.Humanoid.Health > 0 and v:FindFirstChild("Humanoid") and string.find(v.Name,"Boss") and v.Name ~= "rip_indra [Lv. 1500] [Boss]" and v.Name ~= "Ice Admiral [Lv. 700] [Boss]" and v.Name ~= "Don Swan [Lv. 1000] [Boss]" and v.Name ~= "Saber Expert [Lv. 200] [Boss]" and v.Name ~= "Longma [Lv. 2000] [Boss]" 
						and (v.HumanoidRootPart.Position-game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 5000 then -- 50k คือเกาะเงือก
							AnimateText("Unique Kaitan | "..v.Name.." | Distance : "..math.floor(((game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude)))
							repeat wait()
								if Farmtween then Farmtween:Stop() end
								FindBoss = true
								if not game.Players.LocalPlayer.Character:FindFirstChild("HasBuso") then
									game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Buso")
								end
								if game.Players.LocalPlayer:DistanceFromCharacter(v.HumanoidRootPart.Position) > 300 then
									Bosstween = toTarget(v.HumanoidRootPart.CFrame)
								elseif v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 300 then
									if Bosstween then Bosstween:Stop() end
										if AttackRandomType == 1 then
											game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, 30, 1)
										elseif AttackRandomType == 2 then
											game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, 1, 30)
										elseif AttackRandomType == 3 then
											game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(1, 1, -30)
										elseif AttackRandomType == 4 then
											game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(30, 1, 0)
										elseif AttackRandomType == 5 then
											game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(-30, 1, 0)
										else
											game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, 30, 1)
										end
									FastAttack = true
									if not game.Players.LocalPlayer.Character:FindFirstChild(_G.MeleeSelect) then
										wait()
										EquipWeapon(_G.MeleeSelect)
									end
								end
							until not getgenv().Settings.BossAura or v.Humanoid.Health <= 0 or not v.Parent or game.Players.LocalPlayer.Character:WaitForChild("Humanoid"):GetState() == Enum.HumanoidStateType.Dead or stupid
							FindBoss = false
							AnimateText("Unique Kaitan | Kaitan!")
						end
					end
				elseif getgenv().Settings.TypeBossAura == "All Map" and game.Players.LocalPlayer.Data.Level.Value >= tonumber(getgenv().Settings.All_Map_must_level) then
					for i,v in pairs(game.Workspace.Enemies:GetChildren()) do
						if v.Humanoid.Health > 0 and v:FindFirstChild("Humanoid") and string.find(v.Name,"Boss") and v.Name ~= "rip_indra [Lv. 1500] [Boss]" and v.Name ~= "Ice Admiral [Lv. 700] [Boss]" and v.Name ~= "Don Swan [Lv. 1000] [Boss]" and v.Name ~= "Saber Expert [Lv. 200] [Boss]" and v.Name ~= "Longma [Lv. 2000] [Boss]" 
						and (v.HumanoidRootPart.Position-game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 50000 then -- 50k คือเกาะเงือก 
							AnimateText("Unique Kaitan | "..v.Name.." | Distance : "..math.floor(((game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude)))
							repeat wait()
								if Farmtween then Farmtween:Stop() end
								FindBoss = true
								if not game.Players.LocalPlayer.Character:FindFirstChild("HasBuso") then
									game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Buso")
								end
								if game.Players.LocalPlayer:DistanceFromCharacter(v.HumanoidRootPart.Position) > 300 then
									Bosstween = toTarget(v.HumanoidRootPart.CFrame)
								elseif v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 300 then
									if Bosstween then Bosstween:Stop() end
										if AttackRandomType == 1 then
											game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, 30, 1)
										elseif AttackRandomType == 2 then
											game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, 1, 30)
										elseif AttackRandomType == 3 then
											game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(1, 1, -30)
										elseif AttackRandomType == 4 then
											game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(30, 1, 0)
										elseif AttackRandomType == 5 then
											game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(-30, 1, 0)
										else
											game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, 30, 1)
										end
									FastAttack = true
									if not game.Players.LocalPlayer.Character:FindFirstChild(_G.MeleeSelect) then
										wait()
										EquipWeapon(_G.MeleeSelect)
									end
								end
							until not getgenv().Settings.BossAura or v.Humanoid.Health <= 0 or not v.Parent or game.Players.LocalPlayer.Character:WaitForChild("Humanoid"):GetState() == Enum.HumanoidStateType.Dead or stupid
							FindBoss = false
							AnimateText("Unique Kaitan | Kaitan!")
						end
					end
					for i,v in pairs(game:GetService("ReplicatedStorage"):GetChildren()) do
						if string.find(v.Name,"Boss") and v.Name ~= "rip_indra [Lv. 1500] [Boss]" and v.Name ~= "Ice Admiral [Lv. 700] [Boss]" and v.Name ~= "Don Swan [Lv. 1000] [Boss]" and v.Name ~= "Saber Expert [Lv. 200] [Boss]" and v.Name ~= "Longma [Lv. 2000] [Boss]" 
						and (v.HumanoidRootPart.Position-game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 50000 then  -- 50k คือเกาะเงือก 
							AnimateText("Unique Kaitan | "..v.Name.." | Distance : "..math.floor(((game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude)))
							repeat wait()
								if Farmtween then Farmtween:Stop() end
								FindBoss = true
								if not game.Players.LocalPlayer.Character:FindFirstChild("HasBuso") then
									game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Buso")
								end
								if game.Players.LocalPlayer:DistanceFromCharacter(v.HumanoidRootPart.Position) > 300 then
									Bosstween = toTarget(v.HumanoidRootPart.CFrame)
								elseif v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 300 then
									if Bosstween then Bosstween:Stop() end
										if AttackRandomType == 1 then
											game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, 30, 1)
										elseif AttackRandomType == 2 then
											game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, 1, 30)
										elseif AttackRandomType == 3 then
											game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(1, 1, -30)
										elseif AttackRandomType == 4 then
											game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(30, 1, 0)
										elseif AttackRandomType == 5 then
											game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(-30, 1, 0)
										else
											game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, 30, 1)
										end
									FastAttack = true
									if not game.Players.LocalPlayer.Character:FindFirstChild(_G.MeleeSelect) then
										wait()
										EquipWeapon(_G.MeleeSelect)
									end
								end
							until not getgenv().Settings.BossAura or v.Humanoid.Health <= 0 or not v.Parent or game.Players.LocalPlayer.Character:WaitForChild("Humanoid"):GetState() == Enum.HumanoidStateType.Dead or stupid
							FindBoss = false
							AnimateText("Unique Kaitan | Kaitan!")
						end
					end
				end
			end
		end)
	end
end)

-- [Auto Saber]
-- เวล 200 + และน้อยกว่า 400 ทำดาบเช็คในกระเป๋าว่ามีดาบมัั้ยถ้าไม่มีค่อยทำ

function SaberCheck()
	if OldWorld and (not game.Players.LocalPlayer.PlayerGui.Main:FindFirstChild("ChooseTeam") or game.Players.LocalPlayer.PlayerGui.Main.ChooseTeam.Visible == false) and game.Players.LocalPlayer.Data.Level.Value >= 200 then
		if (not game:GetService("Workspace").Enemies:FindFirstChild("Saber Expert [Lv. 200] [Boss]") and not game:GetService("ReplicatedStorage"):FindFirstChild("Saber Expert [Lv. 200] [Boss]")) then
			AnimateText("Unique Kaitan | Saber not Spawned!")
		end 
		if AutofarmLevel and (game:GetService("Workspace").Enemies:FindFirstChild("Saber Expert [Lv. 200] [Boss]") or game:GetService("ReplicatedStorage"):FindFirstChild("Saber Expert [Lv. 200] [Boss]")) then
			checkfarmsaber = true
		end
		if game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Saber") or game.Players.LocalPlayer.Character:FindFirstChild("Saber") then -- เช็คในตัว
			AnimateText("Unique Kaitan | You already have a Saber!")
			getgenv().Settings.Sword.Saber = false
			wait(1)
			AnimateText("Unique Kaitan | Kaitan!")
		end
		for i,v in pairs(game:GetService("ReplicatedStorage").Remotes["CommF_"]:InvokeServer("getInventoryWeapons")) do -- เช็คในกระเป๋า
			for i1,v1 in pairs(v) do
				if v1 == "Saber" then
					AnimateText("Unique Kaitan | You already have a Saber!")
					getgenv().Settings.Sword.Saber = false
					wait(1)
					AnimateText("Unique Kaitan | Kaitan!")
					return true
				end
			end
		end
		if (not game:GetService("Workspace").Enemies:FindFirstChild("Saber Expert [Lv. 200] [Boss]") and not game:GetService("ReplicatedStorage"):FindFirstChild("Saber Expert [Lv. 200] [Boss]")) then
			if FindMob == false and checkfarmsaber == true and game:GetService("Workspace").Map.Jungle.Final.Part.Transparency == 1 then
				AnimateText("Unique Kaitan | Return Autofarm!")
				AutofarmLevel = true
			end
		end
	end
end

spawn(function()
	while wait() do
		if getgenv().Settings.Sword.Saber then
			if game.Players.LocalPlayer.Data.Level.Value > 374 and game.Players.LocalPlayer.Data.Level.Value <= 450 then
				NotfarmSaber = true
			else
				NotfarmSaber = false
			end
		end
	end
end)

spawn(function()
	while wait() do
		if getgenv().Settings.Sword.Saber and SaberCheck() ~= true and NotfarmSaber == false and game.Players.LocalPlayer.Data.Level.Value >= 200 and not game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Saber") and not game.Players.LocalPlayer.Character:FindFirstChild("Saber") 
		and OldWorld and (game:GetService("Workspace").Enemies:FindFirstChild("Saber Expert [Lv. 200] [Boss]") or game:GetService("ReplicatedStorage"):FindFirstChild("Saber Expert [Lv. 200] [Boss]")) then
			if AutofarmLevel == true then 
				repeat wait()
					AutofarmLevel = false 
				until AutofarmLevel == false
			end
			AnimateText("Unique Kaitan | Saber Expert!")
			if game:GetService("Workspace").Map.Jungle.Final.Part.Transparency == 0 then
				if game:GetService("Workspace").Map.Jungle.QuestPlates.Door.Transparency == 0 then
					AnimateText("Unique Kaitan | QuestPlates!")
					toTarget(CFrame.new(-1612.55884, 36.9774132, 148.719543, 0.37091279, 3.0717151e-09, -0.928667724, 3.97099491e-08, 1, 1.91679348e-08, 0.928667724, -4.39869794e-08, 0.37091279))
					if (CFrame.new(-1612.55884, 36.9774132, 148.719543, 0.37091279, 3.0717151e-09, -0.928667724, 3.97099491e-08, 1, 1.91679348e-08, 0.928667724, -4.39869794e-08, 0.37091279).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 4 then
						game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1180.89563, 21.0007095, 187.861374, -0.866141438, -2.23321149e-05, -0.499799222, 2.23321149e-05, 1, -8.33832528e-05, 0.499799222, -8.33832528e-05, -0.866141438)
						wait(1)
						game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1421.19995, 44.5000229, 21.6000061, -0.866039991, -0.499974549, -5.5283308e-06, -5.5283308e-06, 2.06232071e-05, -0.99999994, 0.499974549, -0.866040051, -2.06232071e-05)
						wait(1)
						game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1648.49451, 19.5000286, 437.794678, -0.190788865, -0.981631041, 4.50909138e-05, 4.50909138e-05, -5.47170639e-05, -1, 0.981631041, -0.190788865, 5.47170639e-05)
						wait(1)
						game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1324.10144, 31.4560413, -461.404114, 0.766120374, 2.95190748e-05, 0.642697096, 2.95190748e-05, 1, -8.11179052e-05, -0.642697096, 8.11179052e-05, 0.766120374)
						wait(1)
						game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1152.16479, -1.20998275, -700.431641, -0.995227575, -0.0972457826, -0.0081262216, -0.0181755088, 0.10290432, 0.994525373, -0.0958771482, 0.989926398, -0.104181051)
						wait(1) 
					end
				end
				if game:GetService("Workspace").Map.Jungle.QuestPlates.Door.Transparency ~= 0 then
					AnimateText("Unique Kaitan | Burn!")
					game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("ProQuestProgress", "GetTorch");
					wait(.3)
					game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("ProQuestProgress", "DestroyTorch");
					wait(.4)
					AnimateText("Unique Kaitan | Cup!")
					game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("ProQuestProgress","GetCup")
					wait(0.5)
					if game.Players.LocalPlayer.Character:FindFirstChild("Cup") or game.Players.LocalPlayer.Backpack:FindFirstChild("Cup") then
						EquipWeapon("Cup")
					end
					wait(0.5)
					game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("ProQuestProgress","FillCup",game:GetService("Players").LocalPlayer.Character.Cup)
					wait(0)
					game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("ProQuestProgress","SickMan") 
				end
				if game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("ProQuestProgress","RichSon") == nil then
					game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("ProQuestProgress","RichSon")
				end
				if game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("ProQuestProgress","RichSon") == 0 then
					AnimateText("Unique Kaitan | Checking!")
					if game:GetService("Workspace").Enemies:FindFirstChild("Mob Leader [Lv. 120] [Boss]") or game:GetService("ReplicatedStorage"):FindFirstChild("Mob Leader [Lv. 120] [Boss]") then
						AnimateText("Unique Kaitan | Mob Leader!")
						toTarget(CFrame.new(-2967.59521, -4.91089821, 5328.70703, 0.342208564, -0.0227849055, 0.939347804, 0.0251603816, 0.999569714, 0.0150796166, -0.939287126, 0.0184739735, 0.342634559))
						repeat wait()
							for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
								if v.Name == "Mob Leader [Lv. 120] [Boss]" then
									repeat task.wait()
										StartMagnet = true
										FastAttack = true
										if not game.Players.LocalPlayer.Character:FindFirstChild("HasBuso") then
											game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Buso")
										end
										if not game.Players.LocalPlayer.Character:FindFirstChild(_G.MeleeSelect) then
											wait()
											EquipWeapon(_G.MeleeSelect)
										end
										toTarget(v.HumanoidRootPart.CFrame * CFrame.new(1,20,0))
										PosMon = v.HumanoidRootPart.CFrame
										v.HumanoidRootPart.Size = Vector3.new(80,80,80)
										v.HumanoidRootPart.Transparency = 1
										v.Humanoid.JumpPower = 0
										v.Humanoid.WalkSpeed = 0
										v.HumanoidRootPart.CanCollide = false
										if v.Humanoid:FindFirstChild("Animator") then
											v.Humanoid.Animator:Destroy()
										end
										v.Humanoid:ChangeState(11)
										sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius",  math.huge)
									until v.Humanoid.Health <= 0 or getgenv().Settings.Sword.Saber == false
								end
							end
						until game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("ProQuestProgress","RichSon") == 1
					end
				end
			end
			if game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Relic") then
				AnimateText("Unique Kaitan | Door Lock!")
				repeat wait()
					AnimateText("Unique Kaitan | Relic!")
					EquipWeapon("Relic")
					wait(0.5)
					toTarget(CFrame.new(-1404.91504, 29.9773273, 3.80598116, 0.876514494, 5.66906877e-09, 0.481375456, 2.53851997e-08, 1, -5.79995607e-08, -0.481375456, 6.30572643e-08, 0.876514494))
					wait(1)
				until game:GetService("Workspace").Map.Jungle.Final.Part.Transparency == 1
				AnimateText("Unique Kaitan | Unlock!")
			end
			if game:GetService("Workspace").Map.Jungle.Final.Part.Transparency == 1 then
				if game:GetService("Workspace").Enemies:FindFirstChild("Saber Expert [Lv. 200] [Boss]") or game:GetService("ReplicatedStorage"):FindFirstChild("Saber Expert [Lv. 200] [Boss]") then
					AnimateText("Unique Kaitan | Kill Saber Expert!")
					toTarget(CFrame.new(-1401.85046, 29.9773273, 8.81916237, 0.85820812, 8.76083845e-08, 0.513301849, -8.55007443e-08, 1, -2.77243419e-08, -0.513301849, -2.00944328e-08, 0.85820812))
					for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
						if v.Name == "Saber Expert [Lv. 200] [Boss]" then
							repeat task.wait()
								FastAttack = true
								if not game.Players.LocalPlayer.Character:FindFirstChild("HasBuso") then
									game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Buso")
								end
								if not game.Players.LocalPlayer.Character:FindFirstChild(_G.MeleeSelect) then
									wait()
									EquipWeapon(_G.MeleeSelect)
								end
								toTarget(v.HumanoidRootPart.CFrame * CFrame.new(1,20,0))
								v.HumanoidRootPart.Size = Vector3.new(80,80,80)
								v.HumanoidRootPart.Transparency = 1
								v.Humanoid.JumpPower = 0
								v.Humanoid.WalkSpeed = 0
								v.HumanoidRootPart.CanCollide = false
								if v.Humanoid:FindFirstChild("Animator") then
									v.Humanoid.Animator:Destroy()
								end
								v.Humanoid:ChangeState(11)
								sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius",  math.huge)
								game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("SetSpawnPoint")
							until v.Humanoid.Health <= 0 or getgenv().Settings.Sword.Saber == false or not game:GetService("Workspace").Enemies:FindFirstChild("Saber Expert [Lv. 200] [Boss]")
							AnimateText("Unique Kaitan | Kaitan!")
							if v.Humanoid.Health <= 0 then
								game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("ProQuestProgress","PlaceRelic")
							end
							AutofarmLevel = true
						end
					end
				end
			end
		end
	end
end)


-- [Rengoku]

function RengokuCheck()
	if NewWorld and (not game.Players.LocalPlayer.PlayerGui.Main:FindFirstChild("ChooseTeam") or game.Players.LocalPlayer.PlayerGui.Main.ChooseTeam.Visible == false) and game.Players.localPlayer.Data.Level.Value >= 1350 then
		if game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Rengoku") or game.Players.LocalPlayer.Character:FindFirstChild("Rengoku") then -- เช็คในตัว
			AnimateText("Unique Kaitan | You already have a Rengoku!")
			if RenTween then RenTween:Stop() end 
			getgenv().Settings.Sword.Rengoku = false
			wait(1)
			AnimateText("Unique Kaitan | Kaitan!")
		end
		for i,v in pairs(game:GetService("ReplicatedStorage").Remotes["CommF_"]:InvokeServer("getInventoryWeapons")) do -- เช็คในกระเป๋า
			for i1,v1 in pairs(v) do
				if v1 == "Rengoku" then
					if RenTween then RenTween:Stop() end 
					AnimateText("Unique Kaitan | You already have a Rengoku!")
					getgenv().Settings.Sword.Rengoku = false
					wait(1)
					AnimateText("Unique Kaitan | Kaitan!")
					AutofarmLevel = true
					return true
				end
			end
		end
	end
end

spawn(function()
	while wait() do
		if NewWorld and getgenv().Settings.Sword.Rengoku and RengokuCheck() ~= true and game.Players.LocalPlayer.Data.Level.Value >= 1350 and NewWorld and not game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Rengoku") and not game.Players.LocalPlayer.Character:FindFirstChild("Rengoku") then
			if AutofarmLevel == true then 
				repeat wait()
					AutofarmLevel = false 
				until AutofarmLevel == false
			end
			if game.Players.LocalPlayer.Backpack:FindFirstChild("Hidden Key") or game.Players.LocalPlayer.Character:FindFirstChild("Hidden Key") then
				AnimateText("Unique Kaitan | Unlock!")
				EquipWeapon("Hidden Key")
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(6571.81885, 296.689758, -6966.76514, 0.825126112, 8.412257e-10, 0.564948559, -2.42370835e-08, 1, 3.39100339e-08, -0.564948559, -4.16727595e-08, 0.825126112)
				wait(1)
				AnimateText("Unique Kaitan | Kaitan!")
				FindMob = false
			else
				FindMob = true
				pcall(function()
				if game:GetService("Workspace").Enemies:FindFirstChild("Snow Lurker [Lv. 1375]") or game:GetService("Workspace").Enemies:FindFirstChild("Arctic Warrior [Lv. 1350]") then
					AnimateText("Unique Kaitan | Finding Key!")
					for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
						if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0 then
							if v.Name == "Snow Lurker [Lv. 1375]" or v.Name == "Arctic Warrior [Lv. 1350]" then
								repeat wait()
										if not game.Players.LocalPlayer.Character:FindFirstChild("HasBuso") then
											game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Buso")
										end
										FarmtoTarget = toTarget(v.HumanoidRootPart.CFrame * CFrame.new(0,30,0))
										if v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 300 then
											if FarmtoTarget then FarmtoTarget:Stop() end
											if AttackRandomType == 1 then
												game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, 30, 1)
											elseif AttackRandomType == 2 then
												game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, 1, 30)
											elseif AttackRandomType == 3 then
												game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(1, 1, -30)
											elseif AttackRandomType == 4 then
												game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(30, 1, 0)
											elseif AttackRandomType == 5 then
												game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(-30, 1, 0)
											else
												game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, 30, 1)
											end
											FastAttack = true
											StartMagnet = true
											v.Humanoid.WalkSpeed = 0
											v.Humanoid.PlatformStand = true
											v.HumanoidRootPart.CanCollide = false
											PosMon = v.HumanoidRootPart.CFrame		
											v.Humanoid.NameDisplayDistance = 0			
											if not game.Players.LocalPlayer.Character:FindFirstChild(_G.MeleeSelect) then
												wait()
												EquipWeapon(_G.MeleeSelect)
											end
										end			
								until v.Humanoid.Health <= 0 or game.Players.LocalPlayer.Backpack:FindFirstChild("Hidden Key") or not getgenv().Settings.Sword.Rengoku or not v.Parent
								StartMagnet = false
							end
						end
					end
				else
					AnimateText("Unique Kaitan | Mob not spawned!")
					RenTween = toTarget(CFrame.new(5532.8876953125, 36.51018142700195, -6445.96142578125))
				end
				end)
			end

		end
	end
end)

-- [Fast Attack]

if getgenv().Settings.CameraShaker then
	CameraShaker()
end
if getgenv().Settings.DisabledDamage then
	DisabledDamage()
end

function CheckPlyayers()
	for i,v in pairs(game:GetService("Workspace").Characters:GetChildren()) do
		if v.Name ~= game.Players.LocalPlayer.Name and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 35 then
			return true
		end
	end
end

getgenv().Settings.TypeFastAttack= "Fast (Recommended)" 
coroutine.wrap(function()
	while wait() do
		pcall(function()
			local ac = CombatFrameworkR.activeController
			if ac and ac.equipped then
				if FastAttack then
					if CheckPlyayers() == true then
						AttackFunction()
						AttackPlayersFunction()
						if tick() - cooldownfastattack > tonumber(0.4) then wait(1) cooldownfastattack = tick() end
					else
						AttackFunction()
						AttackPlayersFunction()
						if getgenv().Settings.TypeFastAttack == "Fast (Recommended)" then
							if tick() - cooldownfastattack > tonumber(0.4) then wait(1) cooldownfastattack = tick() end
						elseif getgenv().Settings.TypeFastAttack == "Very Fast" then
							if tick() - cooldownfastattack > tonumber(0.75) then wait(0.1) cooldownfastattack = tick() end
						end
					end
				end
			end
		end)
	end
end)()

-- [Stats Kaitan]
spawn(function()
	while task.wait() do
		if getgenv().Settings.StatsKaitan then
			if game:GetService("Players").LocalPlayer.Data.Stats.Melee.Level.Value < 2400 then
				local args = {
					[1] = "AddPoint",
					[2] = "Melee",
					[3] = Point
				}
				game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
			elseif game:GetService("Players").LocalPlayer.Data.Stats.Defense.Level.Value < 2400 then
				local args = {
					[1] = "AddPoint",
					[2] = "Defense",
					[3] = Point
				}
				game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
			elseif getgenv().Settings.Kaitan.MasteryAndAwaking and game:GetService("Players").LocalPlayer.Data.Stats["Demon Fruit"].Level.Value < 2400 then
				local args = {
					[1] = "AddPoint",
					[2] = "Demon Fruit",
					[3] = Point
				}
				game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
			end
		end
	end
end)

-- [AutoRandomFruit]
spawn(function()
	while wait() do
		if getgenv().Settings.AutoRandomFruit then
			game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Cousin","Buy")
			wait(15)
		end
	end
end) 


function MoveFruit()
	for i,v in pairs(game:GetService("Players").LocalPlayer.Character:GetChildren()) do
		if v:IsA("Tool") and string.find(v.Name, "Fruit") then
			v.Parent = game:GetService("Players").LocalPlayer.Backpack
		end
	end
end

tweenfruit = false
-- [BringFruit]
spawn(function()
	while wait(.1) do
		if getgenv().Settings.BringFruit and RaidStart == false then
			if getgenv().Settings.BringFruitType == "Tween" then
				game.Players.LocalPlayer.PlayerGui.Main.FruitInventory.Position = UDim2.new(10.100, 0, 0.100, 0) -- HideUi
				game.Players.LocalPlayer.PlayerGui.Main.FruitInventory.Visible = true -- เปิดไว้ถึงจะเช็คได้
				local invenfruit = game.Players.LocalPlayer.PlayerGui.Main.FruitInventory.Container.Stored.ScrollingFrame.Frame
				for i,fruitpack in pairs(invenfruit:GetChildren()) do
					if string.find(fruitpack.Name,"-") then
						for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
							pcall(function()
								if string.find(v.Name, "Fruit") and v.Name ~= fruitpack then
									if v:IsA("Tool") then
										wait(1)
										repeat wait()
											tweenfruit = true
											AnimateText("Unique Kaitan | Finding : "..v.Name.." | Distance : "..math.floor(((game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.Handle.Position).Magnitude)))
											AutofarmLevel = false
											getgenv().Settings.Sword.Rengoku = false
											getgenv().Settings.Sword.Saber = false
											wait(1)
										until not getgenv().Settings.BringFruit or not v.Parent or RaidStart == true
										tweenfruit = false
										wait(2)
										AnimateText("Unique Kaitan | Return Autofarm!")
										AutofarmLevel = true
										getgenv().Settings.Sword.Rengoku = true
										getgenv().Settings.Sword.Saber = true
										wait(1)
										AnimateText("Unique Kaitan | Kaitan!")
									end
								end
							end)
						end
						MoveFruit()
					end
				end
			elseif getgenv().Settings.BringFruitType == "Teleport" then
				for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
					if v:IsA("Tool") and string.find(v.Name,"Fruit") then
						firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart,v.Handle,0)  
						wait(10)
					end
				end
			end
		end
	end
end)

function DropFruit()
	pcall(function()
		game.Players.LocalPlayer.PlayerGui.Main.FruitInventory.Position = UDim2.new(10.100, 0, 0.100, 0) -- HideUi
		game.Players.LocalPlayer.PlayerGui.Main.FruitInventory.Visible = true -- เปิดไว้ถึงจะเช็คได้
		local invenfruit = game.Players.LocalPlayer.PlayerGui.Main.FruitInventory.Container.Stored.ScrollingFrame.Frame
		wait(.3)
		for i,v in pairs(invenfruit:GetChildren()) do
			if string.find(v.Name,"-") then
				for _,Backpack in pairs(game:GetService("Players").LocalPlayer.Backpack:GetChildren()) do
					FruitStoreF = string.split(Backpack.Name, " ")[1]
					FruitStoreR = FruitStoreF.."-"..FruitStoreF
					if v.Name == FruitStoreR then
						game:GetService("Players").LocalPlayer.Backpack:FindFirstChild(Backpack.Name):Destroy()
					end												
				end
			end
		end
		for i,v in pairs(invenfruit:GetChildren()) do
			if string.find(v.Name,"-") then
				for _,Character in pairs(game:GetService("Players").LocalPlayer.Character:GetChildren()) do
					FruitStoreF = string.split(Character.Name, " ")[1]
					FruitStoreR = FruitStoreF.."-"..FruitStoreF
					if v.Name == FruitStoreR then
						game:GetService("Players").LocalPlayer.Character:FindFirstChild(Character.Name):Destroy()
					end												
				end
			end
		end
	end)
end

-- [AutoStoreFruit]
spawn(function()
	while wait(.1) do
		if getgenv().Settings.AutoStoreFruit then
			pcall(function()
				DropFruit()
				wait()
